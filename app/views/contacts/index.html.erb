<% @title="Know Your Operating System" %>
<% if current_user.email == 'kowshik16.kk@gmail.com' %>
  <p id="notice"><%= notice %></p>
	<table>
	  <thead>
	    <tr>
	      <th>Name</th>
	      <th>Email</th>
	      <th>Message</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @contacts.each do |contact| %>
	      <tr>
	        <td><%= contact.name %></td>
	        <td><%= contact.email %></td>
	        <td><%= contact.message %></td>
	        <td><%= link_to 'Show', contact %></td>
	        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
	        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	        <% end %>
	      </tr>
	  </tbody>
	</table>
<br />
<% end %>